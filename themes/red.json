{
  "$schema": "vscode://schemas/color-theme",
  "type": "light",
  "name": "HCO-light-saturated",
  "colors": {
    // ======================================================
    // BACKGROUND
    // ======================================================
    // Editor
    "editor.background": "#f0f0f0",
    // Sidebar
    "sideBar.background": "#f0f0f0",
    // Tabs
    "tab.activeBackground": "#f0f0f0",
    // ======================================================
    // FOREGROUND
    // ======================================================
    "editor.foreground": "#1F2328",
    // ======================================================
    // TABS
    // ======================================================
    // Inactive tab
    "tab.inactiveBackground": "#d9d9d9",
    // Tabs bar background
    "editorGroupHeader.tabsBackground": "#cfcfcf",
    // ======================================================
    // SIDEBAR
    // ======================================================
    // Active tab
    "sideBar.border": "#C7C7C7",
    // ======================================================
    // BRACKETS AND BRACKET GUIDES
    // ======================================================
    "editorBracketHighlight.foreground1": "#1F2328",
    "editorBracketHighlight.foreground2": "#1F2328",
    "editorBracketHighlight.foreground3": "#1F2328",
    "editorBracketHighlight.foreground4": "#1F2328",
    "editorBracketPairGuide.activeBackground1": "#000000",
    "editorBracketPairGuide.activeBackground2": "#000000",
    "editorBracketPairGuide.activeBackground3": "#000000",
    "editorBracketPairGuide.activeBackground4": "#000000"
  },
  "tokenColors": [
    {
      "scope": [
        // Comments
				"comment"
      ],
      "settings": {
        "foreground": "#9e9e9e"
      }
    },
    {
      "scope": [],
      "settings": {
        "foreground": "#1F2328"
      }
    },
    {
      "scope": [
        // const, let, function
        "storage",
      ],
      "settings": {
        "foreground": "#bd001f"
      }
    },
    {
      "scope": [
        // new, typeof,
				"keyword",
        // Arrow function's "arrow" character
				"storage.type.function.arrow",
      ],
      "settings": {
        "foreground": "#d73a49"
      }
    },
    {
      "scope": [
        // User function name
				"entity.name.function",
				// CSS func
				"support.function",
      ],
      "settings": {
        "foreground": "#e996a4"
      }
    },
    {
      "scope": [
        // HTML tags
				"entity.name.tag",
				// JSX tags
				"support.class.component"
      ],
      "settings": {
        "foreground": "#0000c7"
      }
    },
    {
      "scope": [
        // Attributes
				"entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#289bbd"
      }
    },
    {
      "scope": [
        
      ],
      "settings": {
        "foreground": "#9d2bca"
      }
    },
    {
      "scope": [
        // Strings
				"string",
				// Markdown: Code in backticks
				"markup.inline.raw"
      ],
      "settings": {
        "foreground": "#3d922a"
      }
    },
    {
      "scope": [],
      "settings": {
        "foreground": "#0451a5"
      }
    },
    {
      "scope": [],
      "settings": {
        "foreground": "#a21319"
      }
    },
    {
      "scope": [],
      "settings": {
        "foreground": "#a89a00"
      }
    },
    {
      "scope": [],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        // return, import, if/else
				"keyword.control",
				// Comments
				"comment",
				// Markdown: italic formatting
				"markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [],
      "settings": {
        "fontStyle": ""
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    // TS: interface type
    // "interface": "#a89a00",
    // TS: generic parameters
    // "typeParameter": "#a89a00",
    // Parameters
    // "parameter.declaration": "#b55908",
  }
}